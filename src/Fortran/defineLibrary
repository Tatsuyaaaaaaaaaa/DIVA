# Definition of the library directory
# ====================================
libraryDir='-I'$divaModuleLib
libraryObj=$divaModuleLib/*.o

export libraryDir
export libraryObj

# Definition of the library directory
# ====================================
libraryMod='-I'$divaModuleMod
libraryPath='-L'$divaModuleLib
libraryLink='-ldiva'$compiler
libraryLink=$libraryLink' -lCommonDiva'$compiler
libraryLink=$libraryLink' -lMath'$compiler
libraryLink=$libraryLink' -lVector'$compiler
libraryLink=$libraryLink' -lMatrix'$compiler
libraryLink=$libraryLink' -lArray'$compiler
#libraryLink=$libraryLink' -lSparseMatrix'$compiler
libraryLink=$libraryLink' -lIO'$compiler
libraryLink=$libraryLink' -lMesh'$compiler
#libraryLink=$libraryLink' -lStabil'$compiler
libraryLink=$libraryLink' -lCalc'$compiler
libraryLink=$libraryLink' -lUtil'$compiler

export libraryMod
export libraryPath
export libraryLink

