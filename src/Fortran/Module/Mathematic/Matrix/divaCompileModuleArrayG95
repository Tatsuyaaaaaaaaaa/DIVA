preproFlag=$preproFlag' -DarrayType=array3DInteger2 -DVARType=INTEGER(KIND=2) '
export preproFlag
moduleName=basicArray3DInteger2

source $divaDir/compileFileG95 templateBasicArray3D.f95 $moduleName

preproFlag='-DtemplatearrayType=array3DInteger4Definition -DtemplateBasicArray=basicArray3DInteger4 -Dtemplate=moduleArray3DInteger4 '
preproFlag=$preproFlag' -DarrayType=array3DInteger4 -DVARType=INTEGER(KIND=4) '
export preproFlag
moduleName=basicArray3DInteger4

source $divaDir/compileFileG95 templateBasicArray3D.f95 $moduleName

preproFlag='-DtemplatearrayType=array3DInteger8Definition -DtemplateBasicArray=basicArray3DInteger8 -Dtemplate=moduleArray3DInteger8 '
preproFlag=$preproFlag' -DarrayType=array3DInteger8 -DVARType=INTEGER(KIND=8) '
export preproFlag
