preproFlag=$preproFlag' -DarrayType=array3DReal8 -DVARType=REAL(KIND=8) '
export preproFlag
moduleName=basicArray3DReal8

source $divaDir/compileFileG95 templateBasicArray3D.f95 $moduleName

preproFlag='-DtemplatearrayType=array3DReal4Definition -DtemplateBasicArray=basicArray3DReal4 -Dtemplate=moduleArray3DReal4 '
preproFlag=$preproFlag' -DarrayType=array3DReal4 -DVARType=REAL(KIND=4) '
export preproFlag
moduleName=basicArray3DReal4

source $divaDir/compileFileG95 templateBasicArray3D.f95 $moduleName

