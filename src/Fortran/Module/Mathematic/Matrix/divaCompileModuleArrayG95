preproFlag='-DtemplateBasicArray=basicArray2DInteger8 -Dtemplate=moduleArray2DInteger8 -DarrayType=array2DInteger8 -DVARType=INTEGER(KIND=8) '
preproFlag=$preproFlag' -DtemplatearrayType=array2DInteger8Definition'
export preproFlag
moduleName=moduleArray2DInteger8

source $divaDir/compileFileG95 templateIntegerArray2D.f95 $moduleName

mv *.mod $divaModuleLib
mv *.o $divaModuleLib


preproFlag='-DtemplatearrayType=array3DReal8Definition -DarrayType=array3DReal8 -DVARType=REAL(KIND=8) '
preproFlag=$preproFlag' -D=_ARRAY_3D_'
export preproFlag
moduleName=array3DReal8Definition

source $divaDir/compileFileG95 moduleArray.f95 $moduleName

preproFlag='-DtemplatearrayType=array3DReal4Definition -DarrayType=array3DReal4 -DVARType=REAL(KIND=4) '
preproFlag=$preproFlag' -D=_ARRAY_3D_'
export preproFlag
moduleName=array3DReal4Definition

source $divaDir/compileFileG95 moduleArray.f95 $moduleName

preproFlag='-DtemplatearrayType=array3DInteger2Definition -DarrayType=array3DInteger2 -DVARType=INTEGER(KIND=2) '
preproFlag=$preproFlag' -D=_ARRAY_3D_'
export preproFlag
moduleName=array3DInteger2Definition

source $divaDir/compileFileG95 moduleArray.f95 $moduleName

preproFlag='-DtemplatearrayType=array3DInteger4Definition -DarrayType=array3DInteger4 -DVARType=INTEGER(KIND=4) '
preproFlag=$preproFlag' -D=_ARRAY_3D_'
export preproFlag
moduleName=array3DInteger4Definition

source $divaDir/compileFileG95 moduleArray.f95 $moduleName

preproFlag='-DtemplatearrayType=array3DInteger8Definition -DarrayType=array3DInteger8 -DVARType=INTEGER(KIND=8) '
preproFlag=$preproFlag' -D=_ARRAY_3D_'
