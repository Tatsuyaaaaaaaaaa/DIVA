preproFlag=$preproFlag' -DtemplatearrayType=array3DReal8Definition'
export preproFlag
moduleName=moduleArray3DReal8

source $divaDir/compileFileG95 templateArray3D.f95 $moduleName

preproFlag='-DtemplateBasicArray=basicArray3DReal4 -Dtemplate=moduleArray3DReal4 -DarrayType=array3DReal4 -DVARType=REAL(KIND=4) '
preproFlag=$preproFlag' -DtemplatearrayType=array3DReal4Definition'
export preproFlag
moduleName=moduleArray3DReal4

source $divaDir/compileFileG95 templateArray3D.f95 $moduleName

preproFlag='-DtemplateBasicArray=basicArray3DInteger2 -Dtemplate=moduleArray3DInteger2 -DarrayType=array3DInteger2 -DVARType=INTEGER(KIND=2) '
preproFlag=$preproFlag' -DtemplatearrayType=array3DInteger2Definition'
export preproFlag
moduleName=moduleArray3DInteger2

source $divaDir/compileFileG95 templateIntegerArray3D.f95 $moduleName

preproFlag='-DtemplateBasicArray=basicArray3DInteger4 -Dtemplate=moduleArray3DInteger4 -DarrayType=array3DInteger4 -DVARType=INTEGER(KIND=4) '
preproFlag=$preproFlag' -DtemplatearrayType=array3DInteger4Definition'
export preproFlag
moduleName=moduleArray3DInteger4

source $divaDir/compileFileG95 templateIntegerArray3D.f95 $moduleName

preproFlag='-DtemplateBasicArray=basicArray3DInteger8 -Dtemplate=moduleArray3DInteger8 -DarrayType=array3DInteger8 -DVARType=INTEGER(KIND=8) '
preproFlag=$preproFlag' -DtemplatearrayType=array3DInteger8Definition'
export preproFlag
moduleName=moduleArray3DInteger8

source $divaDir/compileFileG95 templateIntegerArray3D.f95 $moduleName

mv *.mod $divaModuleLib
mv *.o $divaModuleLib

moduleName=matrixInterface
source $divaDir/compileFileG95 $moduleName.f95 $moduleName

mv *.mod $divaModuleLib
mv *.o $divaModuleLib

